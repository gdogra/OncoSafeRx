{
  "name": "oncosaferx",
  "version": "1.0.0",
  "description": "Oncology drug interaction and recommendation platform",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "npm run build:frontend",
    "start:prod": "NODE_ENV=production node src/index.js",
    "build:frontend": "cd frontend && npm ci && npm run build",
    "start:frontend": "cd frontend && npm start",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "hasura:console": "hasura console --endpoint http://localhost:8081 --admin-secret jrVx4KlySc1PoNUkbn6S41x958VtU6pR9loQHrpe44bSTlYE3qq9NGjGttaZGtR5",
    "db:setup": "echo 'Please run the SQL scripts in database/ folder in your Supabase dashboard'",
    "db:seed": "node src/scripts/seedDatabase.js",
    "sync:rxnorm": "node src/scripts/syncRxNorm.js",
    "sync:cpic": "node src/scripts/syncCPIC.js",
    "migrate:users": "node src/scripts/migrateUsersToSupabase.js"
  },
  "keywords": [
    "oncology",
    "pharmacogenomics",
    "drug-interactions",
    "precision-medicine"
  ],
  "author": "Gautam Dogra",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@sentry/node": "^7.114.0",
    "@sentry/tracing": "^7.114.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.2",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "vite": "^4.5.5"
  }
}
